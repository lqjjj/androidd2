<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.practice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UC1" />
        <activity android:name=".UC2" />
        <activity android:name=".UC3" />
        <activity android:name=".UC4" />
        <activity android:name=".UC5" />
        <activity android:name=".horizontal" />
        <activity android:name=".vertical" />
        <activity android:name=".KeepLeft"></activity>
    </application>

</manifest>